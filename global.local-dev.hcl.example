# global.local-dev.hcl
# 為開發全域變數定義
# 如果你有變數需要讓所有的子模組都需要讀到的話, 需要在這邊定義變數和值

locals {

  # 決定 Azure 地理區域
  primary_location   = "japaneast"
  secondary_location = "japanwest"
  third_location     = "taiwanorth"
  dr_location        = "westus3"

  # 訂閱 ID 列表
  # 如果你是開發人員, 可以填自己的訂閱 ID
  subscription_connectivity_id = ""
  subscription_security_id     = ""
  subscription_identity_id     = ""
  subscription_management_id   = ""
  subscription_devsecops_id    = ""
}
